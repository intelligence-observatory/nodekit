enum JustificationHorizontal : ubyte {
    Left = 1,
    Center,
    Right
}

enum JustificationVertical : ubyte {
    Top = 1,
    Center,
    Bottom
}


table Text {
    text:string;
    font_size:float32;
    justification_horizontal:JustificationHorizontal = 1;
    justification_vertical:JustificationVertical = 1;
    background_color:string;
}